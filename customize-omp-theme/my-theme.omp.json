{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffff00",
          "style": "plain",
          "template": "[\u26a1]",
          "type": "root"
        },
        {
          "foreground": "#18f523",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\uf179 ",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31b ",
            "windows": "\ufab2",
            "wsl": "\ue712"
          },
          "style": "diamond",
          "template": " [{{ if .WSL }}WSL at {{ end }}{{.Icon}}]",
          "trailing_diamond": "<transparent,>\ue0b2</>",
          "type": "os"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#18354c",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,#18354c>\ue0b0</>",
          "properties": {
            "folder_icon": "\uf6d7",
            "folder_separator_icon": "<transparent> \ue0b1 </>",
            "home_icon": "\uf7dd",
            "style": "agnoster_full"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#80ff00",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffff14{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c2b29b{{ end }}",
            "{{ if gt .Ahead 0 }}#4169E1{{ end }}",
            "{{ if gt .Behind 0 }}#4169E1{{ end }}"
          ],
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "\ue0a0 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#f00606",
          "style": "diamond",
          "template": " \uf875 \uf165 \uf876 {{ .Meaning }} \uf876 \uf165 \uf875 ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#505050",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}s ",
          "type": "executiontime"
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "powerline_symbol": "\ue72e",
          "foreground": "#ffffff",
          "background": "#4949ff",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_version": true,
            "prefix": "\ue72e"
          }
        },
        {
          "foreground": "#0ee632",
          "style": "plain",
          "type": "text"
        },
        {
          "properties": {
            "root_icon": "\uf292 "
          },
          "style": "diamond",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#18f523",
          "style": "diamond",
          "template": "  <#ffffff>\ufcbe</> {{ round .PhysicalPercentUsed .Precision }}% ",
          "type": "sysinfo"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "properties": {
            "time_format": " Mon, 2 January | 15:04 "
          },
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#606160",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\uf178",
          "type": "text"
        },
        {
          "foreground": "#606160",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "console_title_template": "{{if .Root}} \u26a1 {{end}}{{ .Shell }} - {{ .Folder }}",
  "version": 2
}